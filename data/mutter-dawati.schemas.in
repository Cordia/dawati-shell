<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/desktop/dawati/toolbar/panels/order</key>
      <applyto>/desktop/dawati/toolbar/panels/order</applyto>
      <owner>mutter-dawati</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[dawati-panel-myzone,dawati-panel-zones,dawati-panel-applications,dawati-panel-status,dawati-panel-people,dawati-panel-internet,dawati-panel-banshee,dawati-panel-devices,dawati-power-icon,dawati-panel-datetime,carrick,bluetooth-panel]</default>
      <locale name="C">
         <short>Order of panels on toolbar</short>
         <long>
		Determines the order of panels on the toolbar.
         </long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/dawati/always_small_screen_mode</key>
      <applyto>/desktop/dawati/always_small_screen_mode</applyto>
      <owner>mutter-dawati</owner>
      <type>bool</type>
      <default>FALSE</default>
      <locale name="C">
         <short>Always use small screen mode</short>
         <long>
		Forces small screen mode to be used even if display size would
                normally imply bigger screen mode.
         </long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
