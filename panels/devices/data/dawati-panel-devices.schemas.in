<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/desktop/dawati/panel-devices/brightness_enabled</key>
      <applyto>/desktop/dawati/panel-devices/brightness_enabled</applyto>
      <owner>dawati-panel-devices</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
        <short/>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/dawati/panel-devices/brightness_value</key>
      <applyto>/desktop/dawati/panel-devices/brightness_value</applyto>
      <owner>dawati-panel-devices</owner>
      <type>float</type>
      <default>1.0</default>
      <locale name="C">
        <short/>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/dawati/panel-devices/brightness_value_battery</key>
      <applyto>/desktop/dawati/panel-devices/brightness_value_battery</applyto>
      <owner>dawati-panel-devices</owner>
      <type>float</type>
      <default>0.75</default>
      <locale name="C">
        <short/>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/dawati/panel-devices/buttons/lid</key>
      <applyto>/desktop/dawati/panel-devices/buttons/lid</applyto>
      <owner>dawati-panel-devices</owner>
      <type>string</type>
      <default>suspend</default>
      <locale name="C">
        <short/>
      </locale>
    </schema>
    <schema>
      <key>/schemas/desktop/dawati/panel-devices/timeout/sleep_computer</key>
      <applyto>/desktop/dawati/panel-devices/timeout/sleep_computer</applyto>
      <owner>dawati-panel-devices</owner>
      <type>int</type>
      <default>300</default>
      <locale name="C">
        <short>Sleep timeout</short>
        <long>Value of -1 means never sleep, other values are seconds
               of continuous idle state before sleeping.</long>
      </locale>
    </schema>
  </schemalist>
</gconfschemafile>
